# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=Retail AI Platform
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered voice calling platform

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_DEBUG_LOGS=true

# Third-party Integrations
VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id
VITE_SENTRY_DSN=your_sentry_dsn_for_error_tracking

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:5000/llm-websocket
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=5
VITE_WEBSOCKET_RECONNECT_INTERVAL=3000

# UI Configuration
VITE_THEME=light
VITE_PRIMARY_COLOR=#3B82F6
VITE_SECONDARY_COLOR=#8B5CF6

# Call Configuration
VITE_MAX_CALL_DURATION=3600
VITE_CALL_TIMEOUT=30000
VITE_ENABLE_CALL_RECORDING=false

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Development Tools
VITE_ENABLE_DEVTOOLS=true
VITE_MOCK_API=false
VITE_LOG_LEVEL=debug

# Build Configuration
VITE_BUILD_SOURCEMAP=true
VITE_BUILD_MINIFY=true

# PWA Configuration (if using PWA features)
VITE_PWA_ENABLED=false
VITE_PWA_NAME=Retail AI
VITE_PWA_SHORT_NAME=RetailAI
VITE_PWA_DESCRIPTION=AI-powered voice calling platform
VITE_PWA_THEME_COLOR=#3B82F6
VITE_PWA_BACKGROUND_COLOR=#ffffff

